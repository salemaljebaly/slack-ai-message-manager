# Copy this file to .env.local and update the values

# CORS Proxy for development (optional)
# You can use https://cors-anywhere.herokuapp.com/ for testing
# Or run your own proxy server
NEXT_PUBLIC_CORS_PROXY=https://cors-anywhere.herokuapp.com/

# Alternative: Use a local proxy server
# NEXT_PUBLIC_CORS_PROXY=http://localhost:8080/

# Set to "true" to show debug information
NEXT_PUBLIC_DEBUG=false